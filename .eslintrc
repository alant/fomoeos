{
  "parser": "babel-eslint",
  "extends": ["eslint:recommended"],
  "globals": {
    "chrome": true
  },
  "env": {
    "browser": true,
    "node": true,
    "jquery": true
  },
  "rules": {
    "arrow-parens": ["error", "always"],
    "no-console": "off"
  },
  "plugins": ["html", "jquery", "babel", "prettier"]
}
